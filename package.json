{
  "name": "petman",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently --kill-others \"npm run _server:run\" \"ng serve --progress=false --proxy-config proxy.conf.json\"",
    "start": "DEBUG=petman node dist/server/bin/www.js",
    "start:universal": "DEBUG=petman UNIVERSAL_APP=true node dist/server/bin/www.js",
    "build": "npm run _client:build && npm run _server:build",
    "build:universal": "npm run _client:build:universal && npm run _server:build",
    "test:client": "ng test",
    "test:server": "echo \"Error: no test specified\" && exit 1",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "e2e:client": "ng e2e",
    "e2e:server": "echo \"Error: no test specified\" && exit 1",
    "compodoc": "./node_modules/.bin/compodoc -d docs -p src/tsconfig.app.json",
    "compodoc:serve": "./node_modules/.bin/compodoc -d docs -s",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon --watch dist/server dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "_client:build": "ng build --prod",
    "_client:build:universal": "ng build --prod && ng build --prod --app petman --output-hashing=none"
  },
  "repository": {
    "url": "https://bitbucket.org/petmanio/petman"
  },
  "private": true,
