{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": ["node"],
    "paths": {
      "@auth/*": ["./app/auth/*"],
      "@adopt/*": ["./app/adopt/*"],
      "@core/*": ["./app/core/*"],
      "@map/*": ["./app/map/*"],
      "@material/*": ["./app/material/*"],
      "@organization/*": ["./app/organization/*"],
      "@shared/*": ["./app/shared/*"],
      "@shelter/*": ["./app/shelter/*"],
      "@walker/*": ["./app/walker/*"],
      "@common/*": ["../common/*"]

    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
